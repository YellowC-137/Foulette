<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foulette">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Foulette"
        android:usesCleartextTraffic="true">
        <activity android:name=".src.Settings.SettingActivity"></activity>
        <activity android:name=".src.Maps.MapActivity" />

        <meta-data
            android:name="com.naver.maps.map.ucpssqqvu5"
            android:value="Qhs3C46OeZo9RFgoeL34J0A75wBHqHZREyFl1ST5" />

        <activity android:name=".src.Result.ResultActivity" />
        <activity android:name=".src.Splash.SplashActivity"
            android:exported="false">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".src.Main.MainActivity"
            android:exported="false">

        </activity>
    </application>

</manifest>